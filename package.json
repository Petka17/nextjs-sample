{
  "name": "nextjs-example",
  "version": "0.0.1",
  "description": "try out combination of nextjs, react hooks, typescript, jest and jsonous",
  "main": "src/index.tsx",
  "repository": "https://github.com/Petka17/nextjs-sample",
  "author": "Petr Klimenko <petrklim@yandex.ru>",
  "license": "MIT",
  "private": false,
  "scripts": {
