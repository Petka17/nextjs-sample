    "express": "4.16.4",
    "next": "8.0.1",
    "react": "16.8.2",
    "react-dom": "16.8.2",
    "react-testing-library": "5.8.0"
  },
  "devDependencies": {
    "@types/jest": "23.3.14",
    "@types/jest-axe": "^2.2.3",
    "babel-plugin-module-resolver": "^3.2.0",
    "jest": "23.6.0",
    "jest-axe": "3.1.1",
    "jest-dom": "3.1.2",
    "ts-jest": "23.10.5",
    "ts-node": "8.0.2",
    "typescript": "3.3.3"
