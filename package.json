{
  "name": "nextjs-example",
  "version": "0.0.1",
  "description": "try out combination of nextjs, react hooks, typescript, jest and jsonous",
  "main": "src/index.tsx",
  "repository": "https://github.com/Petka17/nextjs-sample",
  "author": "Petr Klimenko <petrklim@yandex.ru>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "ts-node server.ts",
    "test": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverall": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "build": "NODE_ENV=production next build src && tsc --allowSyntheticDefaultImports --esModuleInterop --outDir build server.ts",
    "start": "NODE_ENV=production node build/server.js"
  },
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/express-http-proxy": "^1.5.1",
    "@zeit/next-typescript": "^1.1.1",
    "axios": "^0.18.0",
    "express": "4.16.4",
    "express-http-proxy": "^1.5.1",
    "jsonous": "^3.3.1",
    "next": "8.0.3",
    "react": "16.8.3",
    "react-dom": "16.8.3",
    "resulty": "^2.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/jest": "24.0.9",
    "@types/next": "^8.0.1",
    "@types/react": "^16.8.6",
    "@types/react-dom": "16.8.2",
    "@types/zeit__next-typescript": "^0.1.2",
    "babel-plugin-module-resolver": "^3.2.0",
    "coveralls": "^3.0.3",
    "jest": "24.1.0",
    "jest-dom": "3.1.2",
    "react-testing-library": "6.0.0",
    "ts-jest": "24.0.0",
    "ts-node": "8.0.2",
    "typescript": "^3.3.3333"
  }
}
